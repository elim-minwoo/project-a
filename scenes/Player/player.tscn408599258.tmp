script/source = "extends CharacterBody2D

@onready var sprite_2d: AnimatedSprite2D = $AnimatedSprite2D
@onready var player_anim = get_node(\"PlayerAnim\")
@onready var debug_label: Label = $DebugLabel

@export_category(\"Velocity Variables\")
@export var player_speed: float = 330.0
@export var move_speed: float = 330.0
@export var jump_velocity: float = -500.0
@export var fall_accel: float = 1.2
@export var max_fall_speed: float = 1000.0

@export_category(\"Wall Logic Variables\")
@export var wall_x_force = 300.0
@export var wall_y_force = -420.0
@export var wall_slide_speed: float = 30.0

@export_category(\"Dash Variables\")
@onready var dash: Node2D = $Dash
@export var dash_speed = 550.0
@export var dash_duration = 0.4
var bullet_time = true
var can_dash = true

var gravity
var is_jumping := false
var is_sliding := false
var is_wall_jumping := false

# direction
var direction = Input.get_axis(\"moveleft\", \"moveright\")
var look_dir_x: int = 1

# coyote and jump buffer timers
var coyote_timer := 0.0
const COYOTE_TIME = 0.2

var jump_buffer_timer := 0.0
const JUMP_BUFFER_TIME = 0.1

func _process(delta: float) -> void:
	
	if Input.is_action_just_pressed(\"debug_tp\"):
		global_position = Vector2(-500, 0)
	
	
	
	# dash
	if Input.is_action_just_pressed(\"dash\") && dash.can_dash && !dash.is_dashing():
		dash.start_dash(dash_duration)
		
	player_speed = dash_speed if dash.is_dashing() else move_speed

func _physics_process(delta: float) -> void: 
	
	direction = Input.get_axis(\"moveleft\", \"moveright\")
	
	# gravity
	gravity = get_gravity().y
	velocity.y += gravity * delta
	
	var on_wall = is_on_wall_only() and not direction == 0
	
	# input buffer timer
	coyote_timer = max(coyote_timer - delta, 0)
	jump_buffer_timer = max(jump_buffer_timer - delta, 0)
	
	# jump buffer
	if Input.is_action_just_pressed(\"moveup\"):
		jump_buffer_timer = JUMP_BUFFER_TIME
		
	if is_on_floor():
		coyote_timer = COYOTE_TIME # coyote time
		floor_process()
	elif on_wall:
		wall_process()
	#	else:
	#		air_process(delta)
	
	
	
	# horizontal movement
	velocity.x = direction * player_speed

	# vertical movement
	if jump_buffer_timer > 0 and (is_on_floor() or coyote_timer > 0):
		velocity.y = jump_velocity
		is_jumping = true
		jump_buffer_timer = 0.0
		coyote_timer = 0.0
	
	
	
	# jump cut
	if Input.is_action_just_released(\"moveup\") and is_jumping and velocity.y < 0:
		velocity.y *= 0.3
	
	#region fall accel and clamp
	# detect if falling and accerlerate
	elif velocity.y > 0 and not is_on_floor():
		velocity.y += gravity * fall_accel * delta

	# set max falling speed
	velocity.y = clamp(velocity.y, float(-INF), max_fall_speed)
	#endregion
	
	manage_abilities(delta)
	set_animations()
	move_and_slide()
	debug_label.text = str(velocity.x)



#region manage process when on floor, wall, and air
func floor_process():
	is_jumping = false
	
	if Input.is_action_just_pressed(\"moveup\"):
		jump_buffer_timer = JUMP_BUFFER_TIME

func wall_process():

	# wall slide
	velocity.y =  lerp(velocity.y, wall_slide_speed, 0.2)

	# wall jump
	if Input.is_action_just_pressed(\"moveup\"):
		is_jumping = true
		var wall_dir = get_wall_normal().x
		
		velocity.x = wall_dir * -wall_x_force
		velocity.y = wall_y_force
		
		move_and_slide()
		return

# func air_process(delta: float):
#endregion



func manage_abilities():
	# bullet time ability
	if Input.is_action_pressed(\"timeslow\"):
		Engine.time_scale = 0.3
		bullet_time = true
	else:
		Engine.time_scale = 1.0
		bullet_time = false

func manage_flip(direction):
	if direction == 0:
		return
	
	# flip sprite
	if direction == -1.0:
		sprite_2d.flip_h = true
	elif direction == 1.0:
		sprite_2d.flip_h = false
	
	if is_on_wall_only():
		sprite_2d.flip_h = not sprite_2d.flip_h

func set_animations():
	manage_flip(Input.get_axis(\"moveleft\", \"moveright\"))
	
	if is_on_wall_only() and not direction == 0:
		player_anim.play(\"wall\")
	
	# jump anim
	elif is_jumping and velocity.y < 0:
		player_anim.play(\"jump\")
		
	# fall anim
	elif velocity.y > 0 and not is_on_floor():
		player_anim.play(\"fall\")
		
	# run anim
	elif velocity.x != 0 and is_on_floor():
		player_anim.play(\"run\")
		
	# idle anim
	else:
		player_anim.play(\"idle\")
"

