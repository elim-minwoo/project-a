script/source = "extends CharacterBody2D

const ACCEL: float = 22.5
const FRICTION: float = 22.5

# wall jump constants
const GRAVITY_WALL: float = 8.5
const WALL_JUMP_PUSH_FORCE: float = 100.0

# velocity variables
var x_input: float = Input.get_action_strength(\"ui_right\") - Input.get_action_strength(\"ui_left\")
var velocity_weight_x: float = 1.0 - exp( -(ACCEL if x_input else FRICTION) * delta)

@export var move_speed: float = 200.0
@export var jump_velocity: float = -400.0
@export var max_velocity: float = 500.0

@onready var sprite_2d: AnimatedSprite2D = $Sprite2D
@onready var player_anim = get_node(\"PlayerAnim\")
@onready var debug_label: Label = $DebugLabel

var gravity
var is_jumping := false

# coyote and jump buffer timers
var coyote_timer := 0.0
const COYOTE_TIME = 0.2

var jump_buffer_timer := 0.0
const JUMP_BUFFER_TIME = 0.1

var wall_contact_coyote: float = 0.0
const WALL_CONTACT_COYOTE_TIME: float = 0.2

# time left to lock horizontal movement
var wall_jump_lock: float = 0.0
const WALL_JUMP_LOCK_TIME: float = 0.05

# direction facing when contact with wall
var look_dir_x: int = 1

func _physics_process(delta: float) -> void:
	
	# gravity
	gravity = get_gravity().y
	velocity.y += gravity * delta
	
	var on_wall = is_on_wall() and not is_on_floor()
	
	# input buffer timer
	coyote_timer = max(coyote_timer - delta, 0)
	jump_buffer_timer = max(jump_buffer_timer - delta, 0)
	
	# jump buffer
	if Input.is_action_just_pressed(\"moveup\"):
		jump_buffer_timer = JUMP_BUFFER_TIME
		
	if is_on_floor():
		coyote_timer = COYOTE_TIME # coyote time
		floor_process(delta)
	elif on_wall:
		wall_process(delta)
	#	else:
	#		air_process(delta)
		
	# horizontal movement
	var direction = Input.get_axis(\"moveleft\", \"moveright\")
	velocity.x = lerp(velocity.x, x_input * max_velocity, velocity_weight_x)

# vertical movement
	if jump_buffer_timer > 0 and (is_on_floor() or coyote_timer > 0):
		velocity.y = jump_velocity
		is_jumping = true
		jump_buffer_timer = 0.0
		coyote_timer = 0.0
		
# jump cut
	if Input.is_action_just_released(\"moveup\") and is_jumping and velocity.y < 0:
		velocity.y *= 0.3
		
	set_animations()
	move_and_slide()
	debug_label.text = str(velocity.y)

func floor_process(delta: float):
	is_jumping = false
	
	if Input.is_action_just_pressed(\"moveup\"):
		jump_buffer_timer = JUMP_BUFFER_TIME
	
func wall_process(delta: float):
	
	if wall_jump_lock > 0.0:
		wall_jump_lock -= delta
		velocity.x = lerp(velocity.x, x_input * max_velocity, velocity_weight_x * 0.5)
		
	
	if is_on_floor() or wall_contact_coyote > 0.0:
		if Input.is_action_just_pressed(\"moveup\"):
			velocity.y = jump_velocity
			if wall_contact_coyote > 0.0:
				velocity.x = -look_dir_x * WALL_JUMP_PUSH_FORCE
				wall_jump_lock = WALL_JUMP_LOCK_TIME
	
	if !is_on_floor() and velocity.y > 0 and is_on_wall() and velocity.x != 0:
		look_dir_x = sign(velocity.x)
		wall_contact_coyote = WALL_CONTACT_COYOTE_TIME
		velocity.y = GRAVITY_WALL
	else:
		wall_contact_coyote -= delta
		velocity.y +- gravity
	
	# wall slide
	if is_on_wall_only():
		velocity.y = lerp(velocity.y, 150.0, 0.5)
	
	# wall slide
	if is_on_wall_only():
		velocity.y = lerp(velocity.y, 150.0, 0.5)

# func air_process(delta: float):

func manage_flip(direction):
	
	if direction == 0:
		return
	
	# flip sprite
	if direction == -1.0:
		sprite_2d.flip_h = true
	elif direction == 1.0:
		sprite_2d.flip_h = false
	
	if is_on_wall_only():
		sprite_2d.flip_h = not sprite_2d.flip_h

func set_animations():
	manage_flip(Input.get_axis(\"moveleft\", \"moveright\"))
	
	if is_on_wall_only():
		player_anim.play(\"wall\")
	
	# jump anim
	elif is_jumping and velocity.y < 0:
		player_anim.play(\"jump\")
		
	# fall anim
	elif velocity.y > 0 and not is_on_floor():
		player_anim.play(\"fall\")
		
	# run anim
	elif velocity.x != 0 and is_on_floor():
		player_anim.play(\"run\")
		
	# idle anim
	else:
		player_anim.play(\"idle\")
"

