script/source = "extends CharacterBody2D


const SPEED = 150.0
const JUMP_VELOCITY = -400.0

@onready var sprite: AnimatedSprite2D = $AnimatedSprite2D

var is_jumping: bool = false

func _physics_process(delta: float) -> void:
	# Add the gravity.
	if not is_on_floor():
		velocity += get_gravity() * delta
	else:
		is_jumping = false

	# Handle jump start
	if Input.is_action_just_pressed(\"moveup\") and is_on_floor():
		velocity.y = JUMP_VELOCITY
		is_jumping = true

	# jump cut
	if Input.is_action_just_released(\"moveup\") and is_jumping and velocity.y < 0:
		velocity.y *= 0.3 #cuts jump velocity to -50%

	# Horizontal Movement
	var direction := Input.get_axis(\"moveleft\", \"moveright\")
	if direction:
		velocity.x = direction * SPEED
		
		# Flip sprite depending on direction
		sprite.flip_h = true if direction < 0 else false
		
	else:
		velocity.x = move_toward(velocity.x, 0, SPEED)
	
	# play animation depending on movement
	if direction == 0:
		sprite.play(\"idle\")
	else:
		sprite.play(\"run\")

	if not is_on_floor():
		velocity += get_gravity() * delta
		
		if velocity.y > 0:
			velocity.y = clamp(velocity.y, -100000, 500)
		else:
			is_jumping = false

	move_and_slide()
"

